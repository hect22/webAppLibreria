@model IEnumerable<webAppLibreria.Models.Inventario>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Existencia)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdlibroNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdsucursalNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Existencia)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdlibroNavigation.Idlibro)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdsucursalNavigation.Idsucursal)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Idinventario">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Idinventario">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Idinventario">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
